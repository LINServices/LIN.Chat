@code {


    [Parameter]
    public Types.Maps.Models.PlaceDataModel Model { get; set; }


    [Parameter]
    public Action<string, string> OnClick { get; set; }

}

<div class="p-2 bg-gray-100 rounded shadow mt-2" @onclick="()=>OnClick(Model.Latitud, Model.Longitud)">
    <div class="flex flex-col" >
        <label class="text-base gilroy-bold">@Model.Text</label>
        <label class="text-xs">@Model.Longitud | @Model.Latitud</label>
        <label style="margin-top: 3px;" class="text-xs">@Model.Nombre</label>
    </div>
</div>

