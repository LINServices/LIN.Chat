@code {

    [Parameter]
    public Types.Auth.Abstracts.SessionModel<ProfileModel>? e { get; set; } = null;



    private string Img64 => Convert.ToBase64String(e.Account.Perfil);

    [Parameter]
    public Action<Types.Auth.Abstracts.SessionModel<ProfileModel>> OnSelect { get; set; } = (e) =>
    {
        Console.WriteLine("None");
    };

    protected override void OnAfterRender(bool firstRender)
    {
        base.OnAfterRender(firstRender);
    }


}


<div  class="flex hover:shadow-gray-400 bg-white rounded-lg w-full hover:ring-indigo-500 hover:ring-1 items-center">


    <div class="flex p-3 w-full items-center">

        <img class="w-9 h-9 mr-2 p-1 rounded-full ring-2 ring-gray-300 dark:ring-gray-500" src="data:image/png;base64,@Img64" alt="Bordered avatar">

        <div class="flex flex-col">
            <label class="text-sm font-bold">@e.Profile.Alias</label>
            <label class="text-xs -mt-1">@@@e.Account.Usuario</label>
        </div>

    </div>


    <button type="button" class="flex justify-center items-center mr-4 text-white bg-indigo-700 hover:bg-indigo-800 focus:ring-4 focus:outline-none focus:ring-indigo-300 font-medium rounded-full text-sm p-2.5 text-center inline-flex items-center me-2 h-8 w-8">
        <svg xmlns="http://www.w3.org/2000/svg" fill="currentColor" class="w-4 h-4" viewBox="0 0 16 16">
            <path d="M2 0a2 2 0 0 0-2 2v12.793a.5.5 0 0 0 .854.353l2.853-2.853A1 1 0 0 1 4.414 12H14a2 2 0 0 0 2-2V2a2 2 0 0 0-2-2z" />
        </svg>
    </button>
       
</div>
